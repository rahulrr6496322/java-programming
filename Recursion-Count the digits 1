import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {
    static int countD(int num){
        if (num < 0) {
            num = -num;
        }
        if (num < 10) {
            return 1; 
        }
        return 1 + countD(num / 10); 
    }
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int count = countD(num);
        System.out.println("The Count the digits: " + count);
    }
}
