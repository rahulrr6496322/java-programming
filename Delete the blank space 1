import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner obj = new Scanner(System.in);
        String a = obj.nextLine();
        String b = a.replace(" ", "");
        System.out.println(b);
        
    }
}
